{
  "cSpell.words": [
    "Installable",
    "bbcode",
    "Photoshop"
  ],
  "markdownlint.config": {
    "no-inline-html": false,
    "single-trailing-newline": false,
    "no-multiple-blanks": true,
    "no-blanks-blockquote": false,
    "no-trailing-punctuation": false,
    "ol-prefix": false
  },
  "markdownlint.ignore": [
    "_sidebar.md",
    "_404.md"
  ],
  "prettier.printWidth": 100,
  "prettier.endOfLine": "auto",
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": false,
    "editor.rulers": [
      100
    ],
  },
  "files.associations": {
    ".env": "env",
    ".env.example": "env",
    ".excel-formula": "sheets",
    ".formula": "sheets"
  }
}