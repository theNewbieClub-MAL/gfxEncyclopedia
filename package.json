{
  "name": "gfxencyclopedia",
  "version": "1.0.0",
  "scripts": {
    "host": "docsify serve",
    "lint": "markdownlint **/*.md -c config/markdownlint.yaml -i _404.md -i _sidebar.md -i node_modules",
    "proofread": "npm run lint && npm run host",
    "validatelink": "lychee -v -c \"./config/lychee.toml\" \"**/*.md\" \"**/*.html\""
  },
  "keywords": [
    "Encyclopedia"
  ],
  "author": "Natsu Tadama",
  "license": "MIT",
  "devDependencies": {
    "markdownlint-cli": "^0.27.1"
  },
  "dependencies": {
    "docsify-cli": "^4.4.3"
  }
}
